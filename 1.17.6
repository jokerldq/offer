import java.util.ArrayList;
public class Solution {
    public int minNumberInRotateArray(int [] array) {
    	int a=array.length;
        int c=0;
        if(a==0){
            return c;
        }
        for(int i=0;i<a-1;i++){
            if(array[i]>array[i+1]){
                c=array[i+1];
            }
        }
        return c;
    }
}
